<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bonet.example.bonetcalendarviewactivity">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ExampleActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name=".GridCalendarActivity"
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name=".ListCalendarActivity"
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name=".CustomGridCalendarActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="codes.taras.bonetcalendar.material.MaterialGridCalendarActivity"
            android:label="@string/app_name" >
        </activity>
        
    </application>

</manifest>
